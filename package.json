{
  "name": "@applitools/eyes-testcafe",
  "version": "1.1.6",
  "description": "Eyes Testcafe SDK",
  "main": "index.js",
  "scripts": {
    "test": "npm run build:ds && npm run test:mocha && npm run eslint && npm run msg:success || npm run msg:failure",
    "render": "node scripts/render.js",
    "debug": "node scripts/debug.js",
    "test:mocha": "mocha --no-timeouts --trace-warnings 'tests/**/*.test.js'",
    "test:sanity": "mocha --no-timeouts 'tests/!(e2e)/*.test.js'",
    "test:e2e": "mocha --no-timeouts --trace-warnings 'tests/e2e/*.test.js'",
    "test:it": "mocha --no-timeouts 'tests/it/*.test.js'",
    "test:unit": "mocha --no-timeouts 'tests/unit/*.test.js'",
    "msg:success": "notify -t 'Testcafe tests Passed !!' -m 'life`s amazeballs !!!'",
    "msg:failure": "notify -t 'Testcafe tests Failed !!' -m 'Life`s a bitch !!!'",
    "build:ds": "rollup -c rollup.config.js",
    "eslint": "eslint '**/*.js'",
    "prepublishOnly": "npm run build:ds && npm version patch",
    "changelog": "git changelog -p -f v$npm_package_version",
    "changelog:init": "git config changelog.format \"* %s [[%h]($(echo $npm_package_repository_url|cut -d+ -f2|cut -d. -f1-2)/commit/%H)]\"",
    "changelog:install": "sudo apt-get install git-extras"
  },
  "files": [
    "src",
    "index.js",
    "dist"
  ],
  "keywords": [
    "applitools",
    "eyes",
    "testcafe",
    "visual testing",
    "testing",
    "eyes-testcafe"
  ],
  "author": "Daniel Schwartz <daniel.schwartz@applitools.com>",
  "license": "MIT",
  "dependencies": {
    "@applitools/dom-snapshot": "^1.2.24",
    "@applitools/eyes-common": "^3.7.1",
    "@applitools/eyes-sdk-core": "^5.10.0",
    "@applitools/visual-grid-client": "^12.15.1",
    "chalk": "^2.4.2",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-spread": "^7.2.2",
    "core-js-pure": "^3.1.4",
    "eslint": "^5.16.0",
    "eslint-plugin-mocha-no-only": "^1.1.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^2.7.0",
    "express": "^4.17.1",
    "mocha": "^6.1.4",
    "node-notifier-cli": "^1.1.2",
    "prettier": "^1.18.2",
    "rollup": "^1.16.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-replace": "^2.2.0",
    "testcafe": "^1.2.1"
  }
}
