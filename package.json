{
  "name": "eyes-testcafe",
  "version": "1.0.0",
  "description": "Eyes Testcafe SDK",
  "main": "index.js",
  "scripts": {
    "test": "npm run build:ds && npm run test:mocha && npm run eslint",
    "render": "node scripts/render.js",
    "test:debug": "npx testcafe chrome tests/e2e/helloworld.js -L",
    "test:mocha": "mocha --no-timeouts --trace-warnings 'tests/**/*.test.js'",
    "build:ds": "rollup -c rollup.config.js",
    "eslint": "eslint '**/*.js'",
    "prepublishOnly": "npm run build:ds && npm version patch"
  },
  "files": [
    "src",
    "index.js"
  ],
  "keywords": ["applitools", "eyes", "testcafe", "visual testing", "testing", "eyes-testcafe"],
  "author": "Daniel Schwartz <daniel.schwartz@applitools.com>",
  "license": "MIT",
  "dependencies": {
    "@applitools/dom-snapshot": "^1.1.9",
    "@applitools/eyes-common": "^3.4.2",
    "@applitools/eyes-sdk-core": "^5.7.0",
    "@applitools/visual-grid-client": "^12.8.1",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-spread": "^7.2.2",
    "core-js-pure": "^3.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-mocha-no-only": "^1.1.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^2.7.0",
    "express": "^4.16.4",
    "mocha": "^6.1.4",
    "prettier": "^1.17.0",
    "rollup": "^1.11.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "^2.2.0",
    "testcafe": "^1.1.3"
  }
}
